# Vector A: Película
pelicula <- c("Anora",
              "The Brutalist",
              "Flow",
              "Emilia Pérez",
              "Wicked",
              "Dune: Part Two",
              "I’m Still Here")

# Vector B: Premios Oscar 2025
premios_oscar <- c("Mejor Película, Mejor Director, Mejor Actriz, Mejor Guion Original, Mejor Montaje",
                   "Mejor Actor, Mejor Fotografía, Mejor Banda Sonora",
                   "Mejor Película de Animación",
                   "Mejor Actriz de Reparto, Mejor Canción Original",
                   "Mejor Diseño de Vestuario, Diseño de Producción",
                   "Mejor Sonido, Mejores Efectos Visuales",
                   "Mejor Película Internacional")

# Vector C: Mes de estreno
mes_estreno <- c("Enero",
                 "Diciembre",
                 "Diciembre",
                 "Mayo",
                 "Noviembre",
                 "Marzo",
                 "Febrero")

# Vector D: Recaudación mundial (estimada)
recaudacion <- c(41000000,
                 41400000,
                 728000000,
                 714000000,
                 20000000,
                 29900000,
                 30000000)

# Creación del DataFrame
tabla <- data.frame(Pelicula = pelicula,
                    Premios_Oscar_2025 = premios_oscar,
                    Mes_Estreno = mes_estreno,
                    Recaudacion_Mundial = recaudacion)

# Mostrar la tabla
print(tabla)
